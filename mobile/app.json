{
  "expo": {
    "name": "GrooveNomad",
    "slug": "GrooveNomad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aymteb.GrooveNomad"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aymteb.GrooveNomad"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./app/assets/fonts/BwModelica-Bold.otf",
            "./app/assets/fonts/BwModelica-Black.otf",
            "./app/assets/fonts/BwModelica-Hairline.otf",
            "./app/assets/fonts/BwModelica-Light.otf",
            "./app/assets/fonts/BwModelica-Medium.otf",
            "./app/assets/fonts/BwModelica-Regular.otf",
            "./app/assets/fonts/BwModelica-Thin.otf",
            "./app/assets/fonts/Chillax-Bold.otf",
            "./app/assets/fonts/Chillax-Extralight.otf",
            "./app/assets/fonts/Chillax-Light.otf",
            "./app/assets/fonts/Chillax-Medium.otf",
            "./app/assets/fonts/Chillax-Regular.otf",
            "./app/assets/fonts/Chillax-Semibold.otf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./app/assets/images/adaptive-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ]
  }
}
